SELECT house.BUYER_ID, SUM(price.PRICE) AS TOTAL_WORTH FROM house left outer join price on house.HOUSE_ID = price.HOUSE_ID GROUP BY house.BUYER_ID HAVING TOTAL_WORTH >= 100 AND COUNT(house.HOUSE_ID) > 1;


